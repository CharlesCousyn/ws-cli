{
  "name": "ws-cli",
  "version": "1.0.0",
  "description": "Universal WS recorder app for LIARA Laboratory",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "compile": "babel src --out-dir dist",
    "copy": "cpy package.json LICENSE dist",
    "build": "npm-run-all clean copy compile",
    "install": "npm i -g .",
    "start": "npm-run-all copy build install",
    "prod": "node ./dist/index.js",
    "lint": "eslint ./src --ext .js,.json",
    "commit": "git-cz",
    "release": "standard-version",
    "publish": "git push --follow-tags"
  },
  "bin": {
    "ws-cli": "./dist/index.js"
  },
  "keywords": [],
  "author": "FlorentinTh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorentinTh/ws-cli.git"
  },
  "bugs": {
    "url": "https://github.com/FlorentinTh/ws-cli/issues"
  },
  "homepage": "https://github.com/FlorentinTh/ws-cli#readme",
  "pre-commit": [
    "lint"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/eslint-parser": "^7.14.4",
    "@babel/node": "^7.14.2",
    "@babel/plugin-transform-regenerator": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/runtime": "^7.14.0",
    "cpy-cli": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.0",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0"
  },
  "dependencies": {
    "chalk": "^4.1.1",
    "clear": "^0.1.0",
    "clui": "^0.3.6",
    "dayjs": "^1.10.5",
    "figlet": "^1.5.0",
    "js-yaml": "^3.14.1",
    "platform-folders": "^0.5.3",
    "websocket-as-promised": "^2.0.1",
    "ws": "^7.4.6",
    "yargs": "^16.2.0"
  }
}
